<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFAE0"
    android:backgroundTint="#FEFAE0"
    tools:context=".TestResult">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="227dp"
        android:layout_height="135dp"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.359"
        app:srcCompat="@drawable/logotype__2_" />

    <TextView
        android:id="@+id/CongratulationTextView"
        android:layout_width="213dp"
        android:layout_height="37dp"
        android:layout_marginBottom="364dp"
        android:fontFamily="@font/droid_sans"
        android:letterSpacing="0.05"
        android:text="Поздравляем!"
        android:textAlignment="center"
        android:textColor="#606C38"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view3"
        android:layout_width="220sp"
        android:layout_height="3sp"
        android:background="#BC6C25"
        app:layout_constraintBottom_toTopOf="@+id/CongratulationTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.972" />

    <TextView
        android:id="@+id/ResultTextView"
        android:layout_width="39dp"
        android:layout_height="31dp"
        android:layout_marginEnd="1dp"
        android:fontFamily="@font/droid_sans"
        android:letterSpacing="0.05"
        android:text="10"
        android:textAlignment="center"
        android:textColor="#606C38"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ScoreTextView"
        app:layout_constraintTop_toBottomOf="@+id/CongratulationTextView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/word"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:fontFamily="@font/droid_sans"
        android:letterSpacing="0.05"
        android:text="У вас"
        android:textAlignment="center"
        android:textColor="#606C38"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ResultTextView"
        app:layout_constraintEnd_toStartOf="@+id/ResultTextView"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CongratulationTextView"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/view2"
        android:layout_width="220sp"
        android:layout_height="3sp"
        android:background="#BC6C25"
        app:layout_constraintBottom_toTopOf="@+id/buttonOk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ResultTextView"
        app:layout_constraintVertical_bias="0.047" />

    <TextView
        android:id="@+id/ScoreTextView"
        android:layout_width="106dp"
        android:layout_height="29dp"
        android:layout_marginEnd="96dp"
        android:fontFamily="@font/droid_sans"
        android:letterSpacing="0.05"
        android:text="баллов"
        android:textAlignment="center"
        android:textColor="#606C38"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/ResultTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CongratulationTextView"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/buttonOk"
        android:layout_width="239dp"
        android:layout_height="50dp"
        android:backgroundTint="#F7F4E0"
        android:fontFamily="@font/droid_sans"
        android:onClick="Exit"
        android:text="Продолжить"
        android:textAllCaps="false"
        android:textColor="#606C38"
        android:textSize="16sp"
        android:textStyle="bold"
        app:cornerRadius="90sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ResultTextView"
        app:layout_constraintVertical_bias="0.681"
        app:strokeColor="#BC6C25"
        app:strokeWidth="3dp" />

</androidx.constraintlayout.widget.ConstraintLayout>